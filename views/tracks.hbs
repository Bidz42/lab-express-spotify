<div class="con2">

    <div>
            <h1 class="song-name">Song Name + Audio Clip:</h1>
    </div>

    {{#each items}}

    <div class="textthree">
        <div class="p">
            <p>{{this.name}}</p>
        </div>
        <div class="controls">
            <audio controls="" name="media">
                <source src="{{this.preview_url}}" type="audio/mpeg">
            </audio>
        </div>
    </div>

    {{/each}}

</div>